<?xml version="1.0" encoding="UTF-8"?>
<data name="BulletBody" type="struct" >
    <member name="Size" type="float" minvalue="0" maxvalue="1000" />
    <member name="Length" type="float" minvalue="0" maxvalue="1000" />
    <member name="Velocity" type="float" minvalue="0" maxvalue="1000" />
    <member name="ParentVelocityEffect" type="float" default="1" minvalue="-1000" maxvalue="1000" >How hard is the ammunition affected by the parent velocity during spawn.</member>
    <member name="AttachedToParent" type="bool" >Specifies whenever ammunition is attached to the parent ship or ammo. Moving ammo will move in parent's coordinate space</member>
    <member name="Range" type="float" minvalue="0" maxvalue="999999999" />
    <member name="Lifetime" type="float" minvalue="0" maxvalue="999999999" />
    <member name="Weight" type="float" minvalue="0" maxvalue="999999999" />
    <member name="HitPoints" type="int" minvalue="0" maxvalue="999999999" />
    <member name="Color" type="color" />
    <member name="BulletPrefab" type="object" typeid="BulletPrefab" />
    <member name="EnergyCost" type="float" minvalue="0" maxvalue="999999999" />
    <member name="CanBeDisarmed" type="bool" />
    <member name="FriendlyFire" type="bool" />
    <member name="DetonateWhenDestroyed" type="bool" default="true" />
    <member name="AiBulletBehavior" type="enum" typeid="AiBulletBehavior" >Hints for AI and auto-aim0 on usage of this weapon</member>
    
    <!-- obsolete -->
    <member name="Type" type="enum" typeid="BulletTypeObsolete" options="obsolete" />
